{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"jsx": "react-jsx",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"declaration": true,
		"declarationDir": "types",
		"sourceMap": true,
		"outDir": "dist",
		"moduleResolution": "Node",
		"allowSyntheticDefaultImports": true,
		"emitDeclarationOnly": true
	}
}
